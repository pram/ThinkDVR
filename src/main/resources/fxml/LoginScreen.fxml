<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.naughtyzombie.thinkdvr.LoginScreenController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="txtfield" prefWidth="200.0">
                    <VBox.margin>
                        <Insets bottom="50.0" left="10.0" right="10.0" top="50.0" />
                    </VBox.margin>
                </TextField>
                <Button id="changeBtn" mnemonicParsing="false" onAction="#OnWeb" text="change" />
                <WebView fx:id="web" prefHeight="200.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </WebView>
            </children>
        </VBox>
    </children>
</AnchorPane>